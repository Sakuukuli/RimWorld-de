<LanguageData>
  
  <!-- EN: go frenzy -->
  <Frenzy_Go.label>Laufrausch</Frenzy_Go.label>
  <!-- EN: Inspired: Go frenzy -->
  <Frenzy_Go.baseInspectLine>Inspiriert: Laufrausch</Frenzy_Go.baseInspectLine>
  <!-- EN: [PAWN_pronoun] will move faster for the next 8 days. -->
  <Frenzy_Go.beginLetter>[PAWN_pronoun] wird die nächsten 8 Tage schneller laufen.</Frenzy_Go.beginLetter>
  <Frenzy_Go.beginLetterLabel>Laufrausch</Frenzy_Go.beginLetterLabel>
  <!-- EN: {0} is no longer inspired (go frenzy). -->
  <Frenzy_Go.endMessage>{0} ist nicht mehr inspiriert (Laufrausch).</Frenzy_Go.endMessage>
  
